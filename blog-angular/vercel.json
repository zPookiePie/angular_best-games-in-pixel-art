{
    "version": 2,
    "builds": [
      {
        "src": "blog-angular/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "blog-angular/dist/blog-angular/browser"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "blog-angular/dist/blog-angular/browser/index.html"
      }
    ]
  }
  